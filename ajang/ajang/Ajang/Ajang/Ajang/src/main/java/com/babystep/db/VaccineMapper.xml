<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.babystep.db.VaccineMapper">

<select id="babyageselect" resultType="VaccineDTO">
SELECT *
FROM TB_VACCINE V
JOIN TB_BABY B
ON V.VAC_AGE = B.BY_AGE
WHERE B.USER_ID = #{USER ID}
</select>

</mapper>